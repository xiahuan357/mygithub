<div id="contentdetail" class='span12' style="width: 98%; margin-left: 2px;">

	<div class='row-fluid'>
		<div class='span12' style="margin-left: 0">

			<div class='row-fluid' align="center">
				<h2>{{this.model.name}}</h2>
			</div>

			<div style="padding: 5px"></div>
			<div class='row-fluid' align="center">
				发布人：{{this.model.publishedUserEntity.username}} &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
				发布日期：{{this.model.createtime}}
			</div>
			 
            <div class='row-fluid'>
                <div class='span12 blockquote' style="text-indent:0px;color:#333;font-size:14px;line-height:24px;">
                  <div class='box-content'>
                    <strong>摘要： </strong>
					{{this.model.nationalfeature_abstract}}
                  </div>
                </div>
            </div>
            <div class='row-fluid'>
            	<div id="contentWrapperDiv" class="text-center"
					style=" z-index =999; overflow: hidden; margin: auto">
				</div>
            </div>
			
		</div>
	</div>
</div>
