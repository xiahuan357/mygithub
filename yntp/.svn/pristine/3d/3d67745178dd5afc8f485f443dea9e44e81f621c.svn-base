public enum RoomWindowsTypeEnum {
  // 窗户类型：有窗，内窗，无窗
  NONO(0L, "无窗"),
  HAVEWINDOW(1L, "有窗"),
  INTERNALWINDOW(2L, "内窗");

  private final Long id;

  private final String name;

  private RoomWindowsTypeEnum(Long id, String name) {
    this.id = id;
    this.name = name;
  }

  public String getName() {
    return name;
  }

  public Long getId() {
    return id;
  }
}
