<!DOCTYPE html>
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
<meta http-equiv="Access-Control-Allow-Origin" content="*">
<meta content='Flat administration template for Twitter Bootstrap.'
	name='description'>
<meta
	content='width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no'
	name='viewport'>
<meta http-equiv="X-UA-Compatible" content="IE=edge" />
<link rel="shortcut icon" href="/yntp/images/portal/logo/logo.png" />
<title>云南旅游平台</title>
<script type="text/javascript" src="/yntp/js/core/seajs2/dist/sea.js"
	data-config="/yntp/js/global.js" data-main="/yntp/js/main.js"></script>

<!--[if lt IE 9]>
        <script src="js/tool/html5/html5.js" type="text/javascript"></script>
        <![endif]-->
</head>
<body class="wysihtml5-supported ">
	<!-- 头部固定区域 -->
	<div id="top-header"></div>
    <div style="">
    	<!-- 头部中间搜索区域 -->
        <div id="center-header">
        </div>
        
        <!-- 头部导航区域 -->
        <div id="navicat-header" style="min-width:1200px">
        </div>
    </div>
  	<div class="container" style="margin-top: 10px;  width:1170px">
  		<!--搜索区域 和 图片轮播 -->
  		<div class="row-fluid">
  			<div class="span4" id="searchWrapperDiv">
  			</div>
  			<div class="span8" id="carouselWrapperDiv">
  			</div>
		</div>
		
		<!--推荐旅行线路 -->
		<div class='row-fluid' style="margin-top: 10px;">
			<div class='span12   box box-nomargin' id="recommendTourismlineWrapperDiv" >
			</div>
		</div>
		
		<!--线路类型 -->
		<div class='row-fluid' style="margin-top: 10px;">
			<div class='span12 box box-nomargin' id="tourismlineTypeWrapperDiv" >
			</div>
		</div>
		
		<!--线路主题 -->
		<div class="row-fluid" style="margin-top: 10px;">
			<div class='span12 box box-nomargin' id="tourismlineThemeWrapperDiv" >
			</div>
		</div>
		
		<!--线路性质 -->
		<div class='row-fluid' style="margin-top: 10px;">
			<div class='span12 box box-nomargin' id="tourismlinePropertyWrapperDiv"  >
		</div>
		</div>	
	</div>
	
	<hr style=" margin:20px 0;border:0">
	<div id="footer" class="container" style="padding-top: 50px;"></div>
	
	<script type="text/javascript">
            seajs.use('tourismlineindex', function(Index)
            {
                Index.init(
                {
                	topheaderId : "top-header",
                	centerheaderId : "center-header",  
                	navicatheaderId : "navicat-header",
                	
                	searchId : "searchWrapperDiv",
                	carouselId : "carouselWrapperDiv",
                    
                	recommendTourismlineWrapperDiv : "recommendTourismlineWrapperDiv",
                	tourismlineTypeWrapperDiv : "tourismlineTypeWrapperDiv",
                	tourismlineThemeWrapperDiv : "tourismlineThemeWrapperDiv",
                	tourismlinePropertyWrapperDiv : "tourismlinePropertyWrapperDiv",
                    
                    footerId : "footer",
                });
            });
    </script>
</body>
</html>